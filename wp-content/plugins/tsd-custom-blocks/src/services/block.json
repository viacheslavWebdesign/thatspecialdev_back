{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/services",
  "version": "6.6.6",
  "title": "Services",
  "category": "custom-blocks",
  "icon": "smiley",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "services": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "image": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "default": ""
              },
              "alt": {
                "type": "string",
                "default": ""
              }
            }
          },
          "title": {
            "type": "string",
            "default": ""
          },
          "subtitle": {
            "type": "string",
            "default": ""
          },
          "text": {
            "type": "string",
            "default": ""
          }
        }
      }
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "services",
  "editorScript": "file:./index.js"
}
