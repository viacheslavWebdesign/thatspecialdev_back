{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/projects",
  "version": "6.6.6",
  "title": "Projects",
  "category": "custom-blocks",
  "icon": "smiley",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    },
    "projects": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "image": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "default": ""
              },
              "alt": {
                "type": "string",
                "default": ""
              }
            }
          },
          "gif": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "default": ""
              },
              "alt": {
                "type": "string",
                "default": ""
              }
            }
          },
          "link": {
            "type": "string",
            "default": ""
          },
          "repo": {
            "type": "string",
            "default": ""
          },
          "isLive": {
            "type": "boolean",
            "default": false
          }
        }
      }
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "projects",
  "editorScript": "file:./index.js"
}
